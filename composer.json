{
  "name": "dravencms/recaptcha",
  "type": "dravencms-package",
  "description": "DravenCMS Recaptcha",
  "keywords": [
    "dravencms",
    "recaptcha"
  ],
  "license": [
    "LGPL-3.0"
  ],
  "authors": [
    {
      "name": "Adam Schubert"
    }
  ],
  "require": {
    "minetro/recaptcha": "^v2.0"
  },
  "autoload": {
    "psr-0": {
      "Dravencms\\": "src/"
    }
  },
  "extra": {
    "dravencms": {
      "configuration": {
        "extensions": {
          "recaptcha": "Minetro\\ReCaptcha\\DI\\ReCaptchaExtension"
        }
      }
    }
  }
}
